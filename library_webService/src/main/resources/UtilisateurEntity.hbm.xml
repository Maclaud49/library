<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.parlow.library.model.UtilisateurEntity" table="utilisateur" schema="public" catalog="library">
        <id name="id">
            <column name="id" sql-type="integer"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="optimizer">none</param>
                <param name="sequence_name">utilisateur_id_seq</param>
                <param name="increment_size">1</param>
            </generator>
        </id>
        <property name="pseudo">
            <column name="pseudo" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="mdp">
            <column name="mdp" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(60)" length="60"/>
        </property>
        <property name="profil">
            <column name="profil" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>